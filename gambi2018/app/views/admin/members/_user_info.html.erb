<tr>
	<td><%= user.username %></td>
	<td><%= user.password %></td>
	<td><%= user.name %></td>
	<td><%= user.phone %></td>
	<td><%= link_to user.table_id, admin_table_path(user.table) if user.table %></td>
	<td><%= user.table.group_name if user.table %></td>
	<td><%= user.table.status if user.table %></td>
	<td><%= user.table.owner.name if user.table %></td>
	<td><%= user.table.max_people if user.table %></td>
	<td><%= user.table.users.count if user.table %></td>
	<td id="cell-<%= user.id %>"><%= render "notify_#{user.notify_status}", user: user %></td>
</tr>